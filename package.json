{
  "name": "fast-scroll",
  "version": "0.3.0",
  "description": "Navigate faster by increasing your browser's scroll speed",
  "main": "index.js",
  "repository": "git@github.com:diego-aquino/fast-scroll.git",
  "author": "Diego Aquino <diegocruzdeaquino@gmail.com>",
  "license": "GPL-3.0",
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "dev": "yarn build:clean && webpack serve",
    "build": "yarn build:clean && yarn build:check-types && yarn build:run && yarn build:compress",
    "build:clean": "rm -rf build/*",
    "build:check-types": "tsc  --pretty --noEmit",
    "build:run": "cross-env NODE_ENV=production webpack",
    "build:compress": "zx ./dev/build/compress.js",
    "release:upgrade": "zx ./dev/release/upgradeVersion.js",
    "lint": "eslint ./src ./tests ./dev --ext 'js,jsx,ts,tsx'",
    "format": "prettier --write .",
    "test": "jest",
    "test:types": "tsc  --pretty --noEmit",
    "test:coverage": "jest --coverage",
    "pre:commit": "yarn format && yarn lint && yarn test:types",
    "pre:push": "yarn test"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "clsx": "^1.1.1",
    "focus-visible": "^5.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "webextension-polyfill": "^0.8.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.107",
    "@swc/jest": "^0.2.5",
    "@testing-library/dom": "^8.11.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/archiver": "^5.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/webextension-polyfill": "^0.8.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "archiver": "^5.3.0",
    "autoprefixer": "^10.4.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^1.0.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "postcss-focus-visible": "^6.0.1",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.1.15",
    "tailwindcss": "^2.2.17",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "zx": "^4.2.0"
  }
}
